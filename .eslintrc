{
  "extends": [
    "react-app", 
    "plugin:prettier/recommended",  
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "no-console": "warn",
    "quotes": [2, "single", "avoid-escape"],
    "jsx-quotes": [2, "prefer-single"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "jsxSingleQuote": true
      }
    ]
  }
}